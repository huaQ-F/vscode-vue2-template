{
	"name": "vue2-template",
	"displayName": "Vue2 Template",
	"description": "generate code template for vue2 project",
	"version": "0.0.2",
	"publisher": "huaQ-F",
	"repository": {
		"type": "git",
		"url": "https://github.com/huaQ-F/vscode-vue2-template"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.createPage",
		"onCommand:extension.createApiPage"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.createPage",
				"title": "Create Vue2 Page"
			},
			{
				"command": "extension.createApiPage",
				"title": "Create Js Page"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.createPage",
					"group": "1_modification"
				},
				{
					"command": "extension.createApiPage",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"__metadata": {
		"id": "9654d333-7317-4fa7-81b9-f5a610d3d267",
		"publisherId": "24aad3cb-3cef-481e-9a9d-d7864c236ab2",
		"publisherDisplayName": "dabai"
	}
}
